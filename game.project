[project]
title = DefBug

[bootstrap]
main_collection = /main/main.collectionc
render = /game_config/game.renderc

[input]
game_binding = /game_config/game.input_bindingc

[display]
width = 720
height = 1280

[script]
shared_state = 1

[ios]
bundle_identifier = com.jerakin.defbug

[osx]
bundle_identifier = com.jerakin.defbug

[graphics]
texture_profiles = /game_config/game.texture_profiles

[android]
package = com.jerakin.defbug

[library]
include_dirs = /defbug

